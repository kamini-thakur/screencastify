{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Screencastify - Screen Video Recorder",
  "short_name": "Screencastify",
  "description": "__MSG_application_description__",
  "default_locale": "en",
  "version": "1.35.2.177",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "storage",
    "alarms",
    "identity",
    "notifications",
    "unlimitedStorage",
    "desktopCapture",
    "system.cpu",
    "system.memory"
  ],
  "optional_permissions": [
    "activeTab",
    "tabCapture",
    "<all_urls>",
    "webNavigation"
  ],
  "offline_enabled": true,
  "storage": {
    "managed_schema": "managed_schema.json"
  },
  "container": "GOOGLE_DRIVE",
  "api_console_project_id": "1029878751089",
  "background": {
    "scripts": [
      "babel-polyfill.js",
      "bower_components/traceur-runtime/traceur-runtime.min.js",
      "bower_components/angular/angular.min.js",
      "bower_components/chrome-platform-analytics/google-analytics-bundle.js",
      "components/background.js"
    ],
    "persistent": false
  },
  "oauth2": {
    "client_id": "242262968495-vam8ele6sc3j5q90mcn3sd29l2bq5m1s.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/plus.me",
      "email",
      "https://www.googleapis.com/auth/drive.file"
    ]
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+S",
        "chromeos": "Ctrl+Shift+S"
      }
    },
    "toggle-recording": {
      "suggested_key": {
        "default": "Alt+Shift+R"
      },
      "description": "Start/Stop Recording"
    },
    "toggle-pause": {
      "suggested_key": {
        "default": "Alt+Shift+P"
      },
      "description": "Pause/Resume Recording"
    },
    "focus-tab": {
      "suggested_key": {
        "default": "Alt+Shift+F"
      },
      "description": "Change Recording Focus to Current Tab"
    }
  },
  "externally_connectable": {
    "matches": [
      "https://*.screencastify.com/*"
    ]
  },
  "options_page": "app.html#options",
  "minimum_chrome_version": "57",
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' http://localhost:9008 https://www.google-analytics.com https://apis.google.com; object-src 'self'",
  "web_accessible_resources": [
    "components/castifyDraw/release/roboto/*.woff2",
    "components/castifyDraw/release/cam-frame.html",
    "connect.html"
  ],
  "platforms": [{
    "nacl_arch": "arm",
    "sub_package_path": "_platform_specific/arm/"
  }, {
    "nacl_arch": "x86-32",
    "sub_package_path": "_platform_specific/x86-32/"
  }, {
    "nacl_arch": "x86-64",
    "sub_package_path": "_platform_specific/x86-64/"
  }],
  "version_name": "1.35.2-aebe6d4"
}